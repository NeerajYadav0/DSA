static int inSequence(int a, int b, int c){
        int temp = b-a;
        if(a==b)
        {
            return 1;
        }
       if(b>a && c>0 || b<a && c<0)
       {
        if(temp % c== 0)
        {
            return 1;
            
        }
        else
        {
            return 0;
        }
       }
       else
       {
           return 0;
       }
    }
